{{page-title "Mydevices"}}
<table class="channels">
  <tr>
    <th>Device Name:</th>
    <th>Device Type:</th>
    <th>Channel Count:</th>
  </tr>
  {{#each this.mydevices as |device|}}
    <tr>
      <td><a href="/deviceinfo/{{device.id}}">{{device.brand}} {{device.model}}</a></td>
      <td>{{device.deviceType.type}}</td>
      <td>{{device.channels.length}}</td>
    </tr>
  {{/each}}
</table>
{{outlet}}